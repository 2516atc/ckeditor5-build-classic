{
  "compilerOptions": {
    "allowJs": true,
    "declaration": true,
    "declarationDir": "build",
    "lib": [
      "DOM",
      "DOM.Iterable"
    ],
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2019"
  },
  "include": [
    "src"
  ]
}
